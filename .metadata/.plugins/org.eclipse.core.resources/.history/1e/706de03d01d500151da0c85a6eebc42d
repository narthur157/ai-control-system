package server;
//for use with Receive.java

public class Send {	
	public static void main(String[] args) throws IOException {
		PIDLogger logger 		= new PIDLogger();
		BrickComm comm 		  	= new BrickComm();
		PIDController pidCont 	= new PIDController(comm, logger);
		
		pidCont.runTest();
		
		comm.close();
	}
}